        "item": [
          {
            "linkId": "1",
            "text": "Morbilità",
            "answer": [
              {
                "valueString": "ASSENTE o LIEVE"
              }
            ]
          },
          {
            "linkId": "2",
            "text": "Alimentazione",
            "answer": [
              {
                "valueString": "AUTONOMO"
              }
            ]
          },
          {
            "linkId": "3",
            "text": "Alvo e diuresi",
            "answer": [
              {
                "valueString": "INCONTINENZA STABILI per alvo e diuresi (uso pannoloni)"
              }
            ]
          },
          {
            "linkId": "4",
            "text": "Mobilità",
            "answer": [
              {
                "valueString": "CON MINIMO AIUTO (qualche difficoltà)"
              }
            ]
          },
          {
            "linkId": "5",
            "text": "Igiene personale",
            "answer": [
              {
                "valueString": "CON AIUTO MODERATO"
              }
            ]
          },
          {
            "linkId": "6",
            "text": "Stato mentale e comportamento",
            "answer": [
              {
                "valueString": "Collaborante, capace di intendere e di volere"
              }
            ]
          },
          {
            "linkId": "7",
            "text": "Con chi vive",
            "answer": [
              {
                "valueString": "SOLO o COPPIA ma necessita di figure di riferimento (es. figli)"
              }
            ]
          },
          {
            "linkId": "8",
            "text": "Assistenza diretta (Caregiver)",
            "answer": [
              {
                "valueString": "PARZIALMENTE ADEGUATA affidata solo ai familiari, o solo all’assistente familiare, o solo ai servizi territoriali)"
              }
            ]
          }
        ],
